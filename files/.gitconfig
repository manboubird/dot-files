[include]
  path = ~/.gitconfig.local
[pager]
  log = diff-highlight | less
  show = diff-highlight | less
  diff = diff-highlight | less
[color]
  diff = auto
  sh = auto
  status = auto
  branch = auto
  interactive = auto
  ui = auto
[alias]
  cl = clone 
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  dc = diff --cached
  lg = log -p
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  ls = ls-files

